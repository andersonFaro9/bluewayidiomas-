# docker container name
DOCKER_SERVICE_APP=app-quasar
# docker app port
DOCKER_SERVICE_APP_PORT=8080
# the path where the app is deployed
VUE_APP_PUBLIC_PATH="/"
# the name of app
VUE_APP_NAME="Blue Way Idiomas"
# sub title of app
VUE_APP_SUB_TITLE="Blue Way Idiomas"
# the development mode
VUE_APP_DEV=true
# the messages group of language to use
VUE_APP_LOCALE="pt-br"
# the INTL reference locale (https://www.currency-iso.org/dam/downloads/lists/list_one.xml)
VUE_APP_CURRENCY="BRL"

# base URLs
VUE_APP_BASE_URL_REST="http://localhost:8010"
# VUE_APP_BASE_URL_REST="https://blueway.devi.tools"
VUE_APP_BASE_URL_REPORT="http://localhost:8010/report"
VUE_APP_BASE_URL_STATIC="http://localhost:8010/statics"

# Default title
VUE_APP_DEFAULT_TITLE="Blue Way Idiomas"
# Default mask to dates
VUE_APP_DEFAULT_DATE_MASK="DD/MM/YYYY"
# setup Sentry DSN
VUE_APP_SENTRY_DSN=""
# get fake data
VUE_APP_FAKE_DATA=true
# default login used in development mode
VUE_APP_DEFAULT_LOGIN="admin@blueway.com"
# default password used in development mode
VUE_APP_DEFAULT_PASSWORD="aq1sw2de3"
